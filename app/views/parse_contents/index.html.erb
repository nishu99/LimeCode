<div class="container">
  <div class="head">
    <h2>Listing</h2>
  </div>
  <% if @parse_contents.present? %>
    <table class="table">
      <thead>
        <tr>
          <th>Url</th>
          <th>Content</th>        
        </tr>
      </thead>
      <tbody>
        <% @parse_contents.each do |parse_content| %>
          <tr>
            <td><%= parse_content.try(:url) %></td>
            <td><%= parse_content.try(:content) %></td>
          </tr>
        <% end %>      
      </tbody>
    </table>
  <% else %>  
    No data.
  <% end %>
</div>


